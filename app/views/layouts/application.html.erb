<!--CONTACT FORM-->
<div id="contactform"  class="contact flyer">
	<div class="close">X</div>
	<form action="send.php" method="post" >
    	<h4>Send Email</h4>    
    	<p>
    		<input type="text" name="name" id="name" value="" class="input" /> 
    		<label class="smallInput" for="name">Name:</label>
    	</p>
    	<p>
    		<input type="text" name="email" id="email" value="" class="input" /> 
    		<label class="smallInput" for="email">Email:</label>
    	</p>
    	<p>
    		<textarea name="message" id="message" class="input"></textarea>
    		<label class="smallInput" for="message">Message:</label>
    	</p>
    	<input name="subject" id="subject" type="hidden" value="HUD Email Message" />
    	<input name="pagelink" id="pagelink" type="hidden" value="http://www.themolitor.com/hud" />
    	<input name="repemail" id="repemail" type="hidden" value="your.email@email.com" />
    	<input name="send" id="submit_btn" type="submit" class="button" value="Send Message" />
    </form>
</div><!--end contact-->

<!--SEARCH FORM-->
<div id="searching" class="flyer">
	<div class="close">X</div>
	<h4>Search This Site</h4>
	<form method="get" id="searchform" action="">
		<label class="hidden" for="s">Search for:</label>
		<input type="text" value="Search" onfocus="this.value=''; this.onfocus=null;" name="s" id="s" />
        <input type="submit" id="searchsubmit" value="GO!" />    
	</form>
</div><!--end searching-->

