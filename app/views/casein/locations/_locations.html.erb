<%- if @locations && @locations.size>0 -%>
<%= will_paginate(@locations) %>
<%- headers=Array.new 
#headers<<"&nbsp;"
%>


<%- headers<<"Name" -%>

<%- headers<<"Address" -%>

<%- headers<<"Lat" -%>

<%- headers<<"Lng" -%>


<%-
headers<<"Created"
headers<<"Updated"
headers<<"Actions"
-%>

<%= make_table_headers(@locations,headers,{:table_class=>"table",:table_id=>'casein_locations_table'}) %>
<%- @locations.each_with_index do |location,i|-%>
 <tr id="<%= location.dom_id %>">	
   <%= render :partial => 'location', :locals => {:location=>location} %>
</tr>
<% end %>
<%= make_table_footers(headers) %>
<%- else -%>
no locations to display
<%- end -%>



