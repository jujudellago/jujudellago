<%- if video -%>
			<td><%= image_tag("/images/video/#{video.provider}_50.png") %></td>
			<td><%= link_to video.title,casein_video_path(video) %></td>
			<td><%= video.artist %></td>
			<td><%= video.year %></td>
			<td><%= video.created_at.to_s(:db) %></td>
			<td><%= video.updated_at.to_s(:db) %></td>
			<td class="table-actions">
				<%= render :partial => '/shared/enable_disable', :locals => {:object=>video, :enabledisable_path=>enabledisable_casein_video_path(video,:enabled=>(!video.enabled))} %>
  				<%= link_to(get_cms_icon("pencil"), casein_video_path(video),:class=>'with-tip',:title=>'Edit') %>
				<%= link_to(get_cms_icon("cross-circle"), casein_video_path(video), :method => :delete, :confirm => "Are you sure you want to delete this video?",:class=>'with-tip',:title=>'Delete') %>
        	</td>
<%- end -%>