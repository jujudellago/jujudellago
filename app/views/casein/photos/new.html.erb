<!-- Scaffolding generated by Casein v.3.1.7 -->

<h2>Add a new photo to gallery <strong><%= @gallery.name %></strong></h2>

<%= form_for @photo, :url => casein_gallery_photos_path(@gallery), :html => { :method => :post,:multipart => true  } do |f| %>
	
	<%= render :partial => 'casein/photos/form', :locals => { :f => f } %>
	
	<p class="submits">
		<%= link_to 'Cancel', casein_gallery_photos_path(@gallery), :class => 'cancel' %> or
		<%= f.submit "Add new photo", :class => "update" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "table"}Back to list".html_safe, casein_gallery_photos_path(@gallery) %></li>
<% end %>