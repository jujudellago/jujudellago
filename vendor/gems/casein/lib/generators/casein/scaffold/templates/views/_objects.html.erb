<!-- Scaffolding generated by Casein <%= casein_get_full_version_string %> Yabo-concept custom-->
<%%- if @<%= plural_name %> && @<%= plural_name %>.size>0 -%>
<%%= will_paginate(@<%= plural_name %>) %>
<%%- headers=Array.new 
#headers<<"&nbsp;"
%>

<%- for attribute in attributes -%>
<%%- headers<<"<%=attribute.name.humanize%>" -%>
<%- end -%>
<%%-
headers<<"Created"
headers<<"Updated"
headers<<"Actions"
-%>
<%%= make_table_headers(@<%= plural_name %>,headers,{:table_class=>"table",:table_id=>'casein_<%= plural_name %>_table'}) %>
<%%- @<%= plural_name %>.each_with_index do |<%= singular_name %>,i|-%>
 <tr id="<%%= <%= singular_name %>.dom_id %>">	
   <%%= render :partial => '<%= singular_name %>', :locals => {:<%= singular_name %>=><%= singular_name %>} %>
</tr>
<%% end %>
<%%= make_table_footers(headers) %>
<%%- else -%>
no <%= plural_name %> to display
<%%- end -%>



